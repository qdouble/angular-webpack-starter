{
  "name": "angular2webpack2-starter",
  "version": "0.1.0",
  "description": "Angular and Webpack 2 Starter",
  "author": "Qdouble <qdouble@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qdouble/angular2webpack2-starter.git"
  },
  "scripts": {
    "postinstall": "npm run sass",
    "rimraf": "rimraf",
    "webdev": "webpack-dev-server",
    "webdev:hmr": "npm run webdev -- --inline --hot",
    "webpack": "webpack",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json && npm run rimraf -- output",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "protractor": "protractor",
    "pree2e:only": "npm run webdriver:update -- --standalone",
    "e2e": "npm-run-all -p -r e2e:server e2e:only",
    "e2e:only": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e:server": "node prodserver",
    "pretest": "npm run lint",
    "pretest:once": "npm run lint",
    "test:once": "karma start",
    "test": "karma start",
    "ci": "npm run e2e && npm run test:once",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/app/**/*.ts\" ",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist .awcache && npm run clean:compile",
    "clean:dist": "npm run rimraf -- dist .awcache",
    "clean:compile": "npm run rimraf -- \"src/compiled\" ",
    "compile": "npm run compile:aot",
    "compile:aot": "npm run sass && npm run clean:compile && npm run ngc && npm run build:aot",
    "compile:dev": "npm run sass && npm run clean:compile && npm run ngc && npm run build:aot:dev",
    "compile:only": "npm run clean:compile && npm run sass && npm run ngc",
    "compile:watch": "watch-run -i npm run compile:only -p 'src/app/**/*.ts, src/app/**/*.scss' npm run compile:only",
    "prodserver": "node prodserver",
    "sass": "node-sass src -o src --output-style compressed",
    "sass:watch": "node-sass -w src -o src",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:hmr",
    "serve80": "sudo PORT=80 node prodserver",
    "server:aot": "npm-run-all -p -r webdev sass:watch",
    "server:dev": "npm-run-all -p -r webdev sass:watch",
    "server:hmr": "npm-run-all -p -r webdev:hmr sass:watch",
    "server:prod": "npm-run-all -p -r watch:prod prodserver",
    "server:prod80": "npm-run-all -p -r serve80 watch:prod",
    "prebuild": "rm -rf dist && npm run sass",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "build": "npm run build:prod",
    "build:aot": "npm run clean:dist && webpack",
    "build:aot:dev": "npm run clean:dist && webpack",
    "build:dev": "npm run clean:dist && webpack",
    "build:prod": "npm run clean:dist && webpack",
    "watch": "npm run watch:dev",
    "watch:aot": "npm-run-all -p -r \"compile:watch -- --watch\" sass:watch",
    "watch:dev": "npm-run-all -p -r \"build:dev -- --watch\" sass:watch",
    "watch:prod": "npm-run-all -p -r \"build:prod -- --watch\" sass:watch"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "0.6.3",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@angular/router": "3.0.1",
    "@angular/tsc-wrapped": "0.3.0",
    "@angular2-material/button": "2.0.0-alpha.8-2",
    "@angular2-material/card": "2.0.0-alpha.8-2",
    "@angular2-material/core": "2.0.0-alpha.8-2",
    "@angular2-material/icon": "2.0.0-alpha.8-2",
    "@angular2-material/input": "2.0.0-alpha.8-2",
    "@angular2-material/list": "2.0.0-alpha.8-2",
    "@angular2-material/sidenav": "2.0.0-alpha.8-2",
    "@angular2-material/toolbar": "2.0.0-alpha.8-2",
    "@angularclass/hmr": "1.2.1",
    "@angularclass/hmr-loader": "3.0.2",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0-beta.3",
    "@ngrx/router-store": "1.2.4",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.1.0",
    "@ngrx/store-log-monitor": "3.0.2",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "core-js": "2.4.1",
    "express": "4.14.0",
    "methods": "1.1.2",
    "ngrx-store-freeze": "0.1.0",
    "ngrx-store-logger": "0.1.7",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "@types/compression": "~0.0.33",
    "@types/core-js": "~0.9.33",
    "@types/express": "~4.0.33",
    "@types/hammerjs": "~2.0.33",
    "@types/jasmine": "~2.2.34",
    "@types/node": "~6.0.39",
    "@types/protractor": "~1.5.20",
    "@types/selenium-webdriver": "~2.44.29",
    "angular2-router-loader": "~0.2.1",
    "angular2-template-loader": "~0.5.0",
    "awesome-typescript-loader": "~2.2.4",
    "codelyzer": "~0.0.28",
    "compression-webpack-plugin": "~0.3.1",
    "copy-webpack-plugin": "~3.0.1",
    "css-loader": "~0.25.0",
    "es6-promise": "~3.3.0",
    "es6-promise-loader": "~1.0.2",
    "html-webpack-plugin": "~2.22.0",
    "istanbul-instrumenter-loader": "~0.2.0",
    "json-loader": "~0.5.4",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-mocha-reporter": "~2.2.0",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-remap-coverage": "~0.1.1",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "node-sass": "~3.10.0",
    "nodemon": "~1.10.2",
    "npm-run-all": "~3.1.0",
    "phantomjs-polyfill": "~0.0.2",
    "phantomjs-prebuilt": "~2.1.12",
    "protractor": "~4.0.8",
    "raw-loader": "~0.5.1",
    "rimraf": "~2.5.4",
    "source-map-loader": "~0.1.5",
    "string-replace-loader": "~1.0.5",
    "to-string-loader": "~1.1.5",
    "ts-helpers": "~1.1.1",
    "ts-loader": "~0.8.2",
    "ts-node": "~1.3.0",
    "tslint": "~3.15.1",
    "tslint-loader": "~2.1.5",
    "typescript": "~2.0.2",
    "watch-run": "~1.2.4",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "~1.8.1",
    "webpack-dev-server": "2.1.0-beta.4",
    "webpack-merge": "~0.14.1"
  },
